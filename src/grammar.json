{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "bni",
  "rules": {
    "program": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_line"
      }
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "section_line"
        },
        {
          "type": "SYMBOL",
          "name": "key_value_line"
        },
        {
          "type": "SYMBOL",
          "name": "comment_line"
        },
        {
          "type": "SYMBOL",
          "name": "empty_line"
        }
      ]
    },
    "section_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "section"
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "key_value_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "key_value"
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "comment_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "empty_line": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "key_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "key"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "value_list"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "//"
            },
            {
              "type": "PATTERN",
              "value": ".*"
            }
          ]
        }
      }
    },
    "key": {
      "type": "PATTERN",
      "value": "[^\\s=\\[\\];][^=\\[\\];\\n]*"
    },
    "value_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "value"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "value"
              }
            ]
          }
        }
      ]
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "bare_value"
        }
      ]
    },
    "quoted_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "[^\"\\n]"
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "bare_value": {
      "type": "PATTERN",
      "value": "[^,\"\\n]+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[^\\[\\]\\n=;]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}