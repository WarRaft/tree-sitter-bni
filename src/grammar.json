{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "bni",
  "rules": {
    "program": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_line"
      }
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": 10,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "SYMBOL",
                "name": "line_break"
              }
            ]
          }
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "section"
            },
            {
              "type": "SYMBOL",
              "name": "line_break"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "item"
            },
            {
              "type": "SYMBOL",
              "name": "line_break"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "blank"
        }
      ]
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "section_name"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "item": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "key"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "value_list"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "blank": {
      "type": "SYMBOL",
      "name": "line_break"
    },
    "key": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\s=\\[\\];][^=\\[\\];\\n]*"
      }
    },
    "section_name": {
      "type": "PATTERN",
      "value": "[^\\[\\]\\n=;]+"
    },
    "value_list": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "comma"
          },
          {
            "type": "SYMBOL",
            "name": "whitespace"
          },
          {
            "type": "SYMBOL",
            "name": "quoted_string"
          },
          {
            "type": "SYMBOL",
            "name": "unquoted_string"
          }
        ]
      }
    },
    "comma": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": ","
      }
    },
    "whitespace": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[ \\t]+"
      }
    },
    "quoted_string": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[^\"\\n]"
                },
                {
                  "type": "PATTERN",
                  "value": "\"\""
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": "\""
          }
        ]
      }
    },
    "unquoted_string": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\", \\t\\n][^,\\n]*"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "line_break"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}